{
  "name": "@isaque777/metallandmodel",
  "version": "1.0.6",
  "description": "A collection of model classes for TypeScript",
  "keywords": [
    "models",
    "typescript",
    "classes",
    "utilities"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Isaque Soares",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/isaque777/metallandmodel.git"
  },
  "homepage": "https://github.com/isaque777/metallandmodel#readme",
  "bugs": {
    "url": "https://github.com/isaque777/metallandmodel/issues"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "ci": "npm run lint && npm run test:coverage"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
